@model List<GuitarPick.DataLayer.DataModels.Product>
@{
    ViewBag.Title = "Products";
}

<h2>Products</h2>

<table>
    <tr>
        <th>Product Name</th>
        <th>Description</th>
        <th>Price</th>
    </tr>
@foreach (var product in Model)
{
    <tr>
        <td>@product.ProductName</td>
        <td>@product.Description</td>
        <td>@product.Price</td>
    </tr>
}
</table>